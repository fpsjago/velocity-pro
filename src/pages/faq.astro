---
// FAQ page — Velocity Pro
---
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>FAQ — Velocity Pro</title>
</head>
<body>
  <main>
    <section style="text-align:center;padding:5rem 1.5rem 2rem;">
      <h1 style="font-size:clamp(2rem,4vw,3.5rem);font-weight:800;">Frequently Asked Questions</h1>
      <p style="color:#64748b;max-width:560px;margin:1rem auto 0;">Everything you need to know about Velocity Pro.</p>
    </section>

    <div id="faq"></div>
  </main>

  <script>
    import { createElement } from 'react';
    import { createRoot } from 'react-dom/client';
    import PricingFAQ from '../components/Pricing/PricingFAQ';
    import { faqItems } from '../data/faq';

    createRoot(document.getElementById('faq')!).render(
      createElement(PricingFAQ, { items: faqItems, heading: '' })
    );
  </script>
</body>
</html>
